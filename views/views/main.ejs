}
.sidebar{
    margin-top: 0;
    position:fixed;
    left:0;
    width:250px;
    height: 100%;
    background: #042331;
}
.sidebar header{
    font-size: 20px;
    color:white;
    text-align: center;
    line-height: 60px;
    background: #063146;
    user-select: none;
}
.sidebar  li:hover{
    border-top: 1px solid transparent;
    border-bottom: 1px solid transparent;
    box-shadow: 0 0px 10px 3px black;
  }
.sidebar ul a{
    display:block;
    height:100%;
    width:100%;
    line-height: 45px;
    font-size: 13px;
    color:white;
    text-decoration: none;
    padding-left: 40px;
    box-sizing: border-box;
    border-top: 1px solid rgba(255,255,255,.1);
    border-bottom: 1px solid black;
    transition: .4s;
}

ul li:hover a{
    padding-left: 50px;
}
.sidebar ul a i{
    margin-right:16px;
}
#check{
    display:none;
}
.sidebar .icons{
    width: 100%;
    height: 40px;
    text-align: center;
    position: absolute;
    bottom: 100px;
    line-height: 40px;
    display: flex;
    align-items: center;
    justify-content: center;
  }
  .sidebar .icons a{
    height: 100%;
    width: 40px;
    margin-top: 40px;
    display: block;
    margin: 0 5px;
    font-size: 18px;
    color: white;
    background: #042331;
    border-radius: 5px;
    border: 1px solid rgba(255,255,255,0.1);
    transition: .4s;
    transition: all 0.3s ease ;
  }
  .sidebar  .icons a:hover{
    background: #042331;
    border-top: 1px solid transparent;
    border-bottom: 1px solid transparent;
    box-shadow: 0 0px 10px 3px black;
  
  }
  .list-items .icons a:first-child{
    margin-left: 0px;
  }
.dashboardmain{
    background-color: #f0f2f5;
    background-size: cover;
    height: 100%;
    border-top: 0px;
   background-size: cover;
  }
  
  .smallcard{
