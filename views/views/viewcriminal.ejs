    <body>
        <div class="card  m-5">
            <div class="card-header">
                <header class="head1 text-center">Prison Management System</header>
                <header class="head2 text-center">Death Report</header>
         <div class="d-flex flex-row">
            <div >
               <img src="https://upload.wikimedia.org/wikipedia/commons/thumb/b/bc/Unknown_person.jpg/640px-Unknown_person.jpg" class="pic">
            </div>
            <div >
             
                <form >
                    <div class="d-flex flex-row">
                     <div class="d-flex flex-row ml-5" style="margin-right:100px;">
                        <div>
                            <p class="subhead">Criminal_id: &nbsp; &nbsp;</p>
                        </div>
                        <div class="para">
                            <p ><%=sampleData[0].Criminal_id%></p>
                        </div>
                     </div>
                     <div class="d-flex flex-row" style="margin-right:100px;">
                        <div>
                            <p class="subhead">Name: &nbsp; &nbsp; &nbsp; </p>
                        </div>
                        <div class="para">
                            <p ><%=sampleData[0].First_Name%> &nbsp; &nbsp; &nbsp;&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;&nbsp; &nbsp; &nbsp;&nbsp; &nbsp; &nbsp;&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;&nbsp; &nbsp; &nbsp; </p>
                        </div>
                     </div>
                     
                     </div>
                     <div class="d-flex flex-row">
                        <div class="d-flex flex-row ml-5" style="margin-right:100px;">
                            <div>
                                <p class="subhead">Inmate_since:&nbsp; &nbsp; &nbsp;&nbsp;</p>
                            </div>
                            <div class="para">
                                <p><%=sampleData[0].inmate_since%></p>
                            </div>
                        </div>
                     </div>
                     <div class="d-flex flex-row">
                        <div class="d-flex flex-row ml-5" style="margin-right:100px;">
                            <div>
                                <p class="subhead">Gender:&nbsp; &nbsp; &nbsp;&nbsp;</p>
                            </div>
                            <div class="para">
                                <p><%=sampleData[0].Gender%></p>
                            </div>
                        </div>
                        <div class="d-flex flex-row ">
                            <div>
                                <p class="subhead">Subjail_no:&nbsp; &nbsp; &nbsp;&nbsp;</p>
                            </div>
                            <div class="para">
                                <p><%=sampleData[0].Subjail_no%></p>
                            </div>
                        </div>
                     </div>
                     <div class="d-flex flex-row">
                        <div class="d-flex flex-row ml-5" style="margin-right:100px;">
                            <div>
                                <p class="subhead">Eye_color:&nbsp; &nbsp; &nbsp;&nbsp;</p>
                            </div>
                            <div class="para">
                                <p><%=sampleData[0].eye_color%></p>
                            </div>
                        </div>
                        <div class="d-flex flex-row " style="margin-right:100px;">
                            <div>
                                <p class="subhead">Marital_status:&nbsp; &nbsp; &nbsp;&nbsp;</p>
                            </div>
                            <div class="para">
                                <p><%=sampleData[0].Marital_status%></p>
                            </div>
                        </div>
                        <div class="d-flex flex-row" >
                            <div>
                                <p class="subhead">Age: &nbsp; &nbsp; &nbsp;&nbsp;</p>
                            </div>
                            <div class="para">
                                <p ><%=sampleData[0].Age%></p>
                            </div>
                         </div>
                     </div>
                    
                
                
        <div >
            <h1 class="subhead2"> Case Details</h1>
            
                <div class="d-flex flex-row" >
                    <div>
                        <p class="subhead">Crime_id:&nbsp; &nbsp; &nbsp;&nbsp;</p>
                    </div>
                    <div class="para">
                        <p><%=sampleData[0].Crime_id%> &nbsp; &nbsp; &nbsp;&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;&nbsp; &nbsp; &nbsp;&nbsp; &nbsp; &nbsp;&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;&nbsp; &nbsp; &nbsp;</p>
                    </div>
                </div>
                <div class="d-flex flex-row " >
                    <div>
                        <p class="subhead">Crime_type:&nbsp; &nbsp; &nbsp;&nbsp;</p>
                    </div>
                    <div class="para">
                        <p><%=sampleData[0].Crime_type%>&nbsp; &nbsp; &nbsp;&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;&nbsp; &nbsp; &nbsp;&nbsp; &nbsp; &nbsp;&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;&nbsp; &nbsp; &nbsp;</p>
                    </div>
                </div>
                <div class="d-flex flex-row" >
                    <div>
                        <p class="subhead">Years_of_prison: &nbsp; &nbsp; &nbsp;&nbsp;</p>
                    </div>
                    <div class="para">
                        <p ><%=sampleData[0].Years_of_prison %>&nbsp; &nbsp; &nbsp;&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;&nbsp; &nbsp; &nbsp;&nbsp; &nbsp; &nbsp;&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;&nbsp; &nbsp; &nbsp;</p>
                    </div>
                 </div>
                 <div class="d-flex flex-row" >
                    <div>
                        <p class="subhead">Death_reason: &nbsp; &nbsp; &nbsp;&nbsp;</p>
                    </div>
                    <div class="para">
                        <p ><%=sampleData[0].Death_reason %>&nbsp; &nbsp; &nbsp;&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;&nbsp; &nbsp; &nbsp;&nbsp; &nbsp; &nbsp;&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;&nbsp; &nbsp; &nbsp;</p>
                    </div>
                 </div>
             
        </div>
            
                   
        </form>
       
            </div>
            </div>
