        <title></title>
        <style>
            .heading{
                font-size: 25px;
    
                font-weight: 600;
            }
            body{
                background: url(http://wrbbradio.org/wp-content/uploads/2016/10/grey-background-07.jpg) ;
                
            }
            .pic{
                height:160px;
            }
            .head1{
                font-size: 18px;
                font-weight: 500;
               
            }
            .head2{
                font-size: 18px;
                font-weight: 500;
                margin-bottom:25px;
            }
            .para{
                border-width: 1px;
                border-color: rgb(92, 90, 90);
                border-style: solid;
                height:30px;
                padding-left: 5px;
                padding-right: 100px;
            }
            .subhead{
                border-width: 1px;
                border-color: rgb(92, 90, 90);
                background-color: rgb(153, 149, 149);
                border-style: solid;
                padding-left: 5px;
                height:30px;
                color:white
            }
